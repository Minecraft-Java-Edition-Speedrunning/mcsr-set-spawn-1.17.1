{
  "schemaVersion": 1,
  "id": "setspawnmod",
  "version": "${version}",

  "name": "Set Spawn Mod",
  "description": "Sets the player spawnpoint in SSG to specified coordinates.",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/setspawnmod/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "net.set.spawn.mod.SetSpawn"
    ]
  },
  "mixins": [
    "setspawnmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.2",
    "minecraft": "~1.17",
    "java": ">=17"
  },
  "breaks": {
    "worldpreview": "*"
  }
}
